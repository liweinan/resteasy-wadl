    <param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="p2" style="template" type="xs:string"/>
    <method id="mixed" name="GET">
        <request>
            <param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="h1" style="header" type="xs:string"/>
            <param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="Cookie" style="header" type="xs:string" path="c1"/>
            <param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="q1" style="query" type="xs:string"/>
            <param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="h2" style="header" type="xs:string"/>
            <param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="Cookie" style="header" type="xs:string" path="c2"/>
            <param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="q2" style="query" type="xs:string"/>
        </request>
        <response>
            <representation mediaType="text/plain"/>
        </response>
    </method>
</resource>


